<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ConstraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondoreal"
    tools:context=".FlashLevel">


    <ImageButton
        android:id="@+id/doriButton"
        android:layout_width="57dp"
        android:layout_height="58dp"
        android:background="@null"
        android:onClick="alTocarDori"
        android:scaleType="centerInside"
        app:srcCompat="@drawable/dori"
        tools:layout_editor_absoluteY="409dp" />

    <ImageButton
        android:id="@+id/callaboButton"
        android:layout_width="57dp"
        android:layout_height="58dp"
        android:background="@null"
        android:scaleType="centerInside"
        android:onClick="alTocarCallabo"
        app:srcCompat="@drawable/calalbo"
        tools:layout_editor_absoluteY="351dp" />

    <ImageButton
        android:id="@+id/ballenaButton"
        android:layout_width="161dp"
        android:layout_height="157dp"
        android:background="@null"
        android:onClick="alTocarBallena"
        android:scaleType="centerInside"
        app:srcCompat="@drawable/ballena"
        tools:layout_editor_absoluteY="136dp" />

    <ImageButton
        android:id="@+id/blanquimoraoButton"
        android:layout_width="57dp"
        android:layout_height="58dp"
        android:background="@null"
        android:onClick="alTocarBlanquimorao"
        android:scaleType="centerInside"
        app:srcCompat="@drawable/blanquimorao"
        tools:layout_editor_absoluteY="293dp" />

    <ImageButton
        android:id="@+id/pezAmarilloNaranja"
        android:layout_width="95dp"
        android:layout_height="79dp"
        android:background="@null"
        android:onClick="alTocarAmarillo"
        android:scaleType="centerInside"
        app:srcCompat="@drawable/amarillonaranja"
        tools:layout_editor_absoluteY="57dp" />

    <ImageView
        android:id="@+id/contador"
        android:layout_width="68dp"
        android:layout_height="57dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/nuevenueve" />

</androidx.constraintlayout.widget.ConstraintLayout>